# /etc/systemd/system/pirover.service
[Unit]
Description=PiRover webapp

[Service]
SuccessExitStatus=143
ExecStart=/usr/bin/java -jar -Dserver.port=8090 -Dspring.profiles.active=pi -jar /home/pi/pz-web-api-1.0.0-SNAPSHOT.jar
Type=simple
User=pi
Group=pi

[Install]
WantedBy=multi-user.target